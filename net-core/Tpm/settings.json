{
  "pluginsDir": "plugins",
  "logLevel": "DEBUG",
  "dataDir": "data",
  "apiPort": 8081,
  "protocols": {
    "http-01": {
      "protocol": "http",
      "http": 80,
      "https": 443,
      "proxy": 9000,
      "ssl": {
        "der": "resource://certificates/ca.der",
        "key": "resource://certificates/ca.key",
        "cname": "C=US,O=Local Development,CN=local.org",
        "hosts": [
        ]
      },
      "plugins": {
        "mock-plugin": {

        },
        "rate-limit-plugin": {
          "target": [
            "@(net-core-rest|localhost|127.0.0.1)\\/api/tasks.*"
          ],
          "rateLimit": 120,
          "warningThresholdPercent": 80,
          "resetTimeWindowSeconds": 60
        },
        "latency-plugin": {
          "target": [
            "@(net-core-rest|localhost|127.0.0.1)\\/api/tasks.*"
          ],
          "minMs": 50,
          "maxMs": 5000
        },
        "error-plugin": {
          "target": [
            "@(net-core-rest|localhost|127.0.0.1)\\/api/tasks.*"
          ],
          "showError": 500,
          "errorMessage": "Random Generated Error",
          "percentAction": 70
        },
        "record-plugin": {
          "target": [
            "@(net-core-rest|localhost|127.0.0.1)\\/api/tasks.*"
          ]
        },
        "replay-plugin": {
          "target": [
            "@(net-core-rest|localhost|127.0.0.1)\\/api/tasks.*"
          ]
        },
        "rewrite-plugin": {
          "rewritesFile": "rewrite.json"
        }
      }
    },
    "mysql-01": {
      "protocol": "mysql",
      "port": 3306,
      "login": "root",
      "password": "password",
      "timeout": 30,
      "connectionString": "jdbc:mysql://net-core-mysql:3306/db",
      "plugins": {
        "record-plugin": {},
        "replay-plugin": {}
      }
    }
  }
}
