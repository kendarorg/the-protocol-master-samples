{
  "pluginsDir": "plugins",
  "logLevel": "DEBUG",
  "dataDir": "data",
  "apiPort": 8081,
  "protocols": {
    "http-01": {
      "protocol": "http",
      "http": 80,
      "https": 443,
      "proxy": 9000,
      "ssl": {
        "der": "resource://certificates/ca.der",
        "key": "resource://certificates/ca.key",
        "cname": "C=US,O=Local Development,CN=local.org",
        "hosts": [
        ]
      },
      "plugins": {
        "record-plugin": {
          "recordSites": [
            "java_http",
            "java_rest"
          ]
        },
        "replay-plugin": {
          "matchSites": [
            "java_rest"
          ]
        },
        "rewrite-plugin": {
          "rewritesFile": "rewrite.json"
        }
      }
    },
    "mqtt-01": {
      "protocol": "mqtt",
      "port": 1883,
      "login": "",
      "password": "",
      "timeout": 30,
      "connectionString": "tcp://mosquitto_java:1883",
      "plugins": {
        "record-plugin": {},
        "replay-plugin": {}
      }
    },
    "mysql-01": {
      "protocol": "mysql",
      "port": 3306,
      "login": "root",
      "password": "password",
      "timeout": 30,
      "connectionString": "jdbc:mysql://db_mysql_java:3306",
      "plugins": {
        "record-plugin": {},
        "replay-plugin": {}
      }
    },
    "mongodb-01": {
      "protocol": "mongodb",
      "port": 27017,
      "login": "remotelogin",
      "password": "remotepassword",
      "timeout": 30,
      "connectionString": "tcp://localhost:1885",
      "plugins": {
        "record-plugin": {},
        "replay-plugin": {}
      }
    }
  }
}
