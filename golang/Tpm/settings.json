{
  "pluginsDir": "plugins",
  "logLevel": "DEBUG",
  "dataDir": "data",
  "apiPort": 8081,
  "protocols": {
    "http-01": {
      "protocol": "http",
      "http": 80,
      "https": 443,
      "proxy": 9000,
      "ssl": {
        "der": "resource://certificates/ca.der",
        "key": "resource://certificates/ca.key",
        "cname": "C=US,O=Local Development,CN=local.org",
        "hosts": [
        ]
      },
      "plugins": {
        "record-plugin": {
          "recordSites": [
            "go-http"
          ]
        },
        "replay-plugin": {
          "matchSites": [

          ]
        },
        "rewrite-plugin": {
          "rewritesFile": "rewrite.json"
        }
      }
    },
    "redis-01": {
      "protocol": "redis",
      "port": 6379,
      "login": "",
      "password": "",
      "timeout": 30,
      "connectionString": "tcp://go-redis:6379",
      "plugins": {
        "record-plugin": {
          "resetConnectionsOnStart": true
        },
        "replay-plugin": {
          "resetConnectionsOnStart": true,
          "respectCallDuration": true
        }
      }
    },
    "postgres-01": {
      "protocol": "postgres",
      "port": 5432,
      "login": "postgres",
      "password": "password",
      "timeout": 30,
      "connectionString": "jdbc:postgres://go-postgres:5432/db",
      "plugins": {
        "record-plugin": {},
        "replay-plugin": {}
      }
    }
  }
}
